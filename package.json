{
  "name": "inp-http-response",
  "version": "0.0.1",
  "description": "HttpResponse class for InP communication with utility and factory functions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "clean": "rimraf .nyc_output coverage dist",
    "clean:modules": "rimraf node_modules package-lock.json && npm install",
    "test": "mocha --require ts-node/register --require __tests__/mochaSetup.ts __tests__/**/*.spec.ts",
    "test:watch": "mocha --require ts-node/register --require __tests__/mochaSetup.ts --watch-extensions ts --watch __tests__/**/*.spec.ts",
    "test:coverage": "nyc npm test",
    "lint": "eslint . --ext .js,.ts"
  },
  "nyc": {
    "reporter": [
      "text",
      "html",
      "json"
    ],
    "exclude": [
      "__tests__/**/*"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "keywords": [],
  "author": "CDI AI team",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "abab": "^2.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "mocha": "^9.1.1",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^4.4.2"
  },
  "contributors": [
    "Vojtech.Soban@citrix.com"
  ],
  "dependencies": {
    "inp-commons": "0.0.1"
  }
}
